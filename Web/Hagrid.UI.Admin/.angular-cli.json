{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"project": {
		"name": "hagrid-ui-admin"
	},
	"apps": [
		{
			"name": "accounts",
			"root": "src",
			"outDir": "../../publish/accounts/Hagrid.UI.Admin",
			"assets": [
				{
					"input": "./assets/img",
					"glob": "**/*",
					"output": "./assets/img"
				},
				"web.config",
				{
					"input": "./",
					"glob": "web.config",
					"output": "."
				}
			],
			"index": "index.html",
			"main": "main.ts",
			"polyfills": "polyfills.ts",
			"prefix": "app",
			"styles": [
				"../node_modules/bootstrap/dist/css/bootstrap.css",
				"../node_modules/font-awesome/css/font-awesome.css",
				"assets/less/default.theme.less",
				"../node_modules/icheck/skins/flat/red.css",
				"../node_modules/bootstrap-datepicker/dist/css/bootstrap-datepicker3.css",
				"../node_modules/admin-lte/plugins/daterangepicker/daterangepicker.css",
				"../node_modules/bootstrap3-wysihtml5-bower/dist/bootstrap3-wysihtml5.css",
				"../node_modules/jsoneditor/dist/jsoneditor.min.css",
				"assets/css/styles.css"
			],
			"scripts": [
				"../node_modules/jquery/dist/jquery.js",
				"../node_modules/jqueryui/jquery-ui.js",
				"../node_modules/bootstrap/dist/js/bootstrap.js",
				"../node_modules/moment/moment.js",
				"../node_modules/daterangepicker/daterangepicker.js",
				"../node_modules/bootstrap-datepicker/js/bootstrap-datepicker.js",
				"../node_modules/bootstrap3-wysihtml5-bower/dist/bootstrap3-wysihtml5.all.js",
				"../node_modules/icheck/icheck.js",
				"../node_modules/admin-lte/dist/js/app.js",
				"../node_modules/jsoneditor/dist/jsoneditor.min.js",
				"assets/js/adminLTEOptions.js",
				"assets/js/scripts.js"
			],
			"environmentSource": "environments/environment.ts",
			"environments": {
				"dev": "environments/environment.ts",
				"desenv": "environments/environment.desenv.ts",
				"hom": "environments/environment.hom.ts",
				"staging": "environments/environment.staging.ts",
				"sandbox": "environments/environment.sandbox.ts",
				"prod": "environments/environment.prod.ts"
			}
		},
		{
			"name": "manager",
			"root": "src",
			"outDir": "../../publish/manager/Hagrid.UI.Manager.Admin",
			"assets": [
				{
					"input": "./assets/img",
					"glob": "**/*",
					"output": "./assets/img"
				},
				"web.config",
				{
					"input": "./",
					"glob": "web.config",
					"output": "."
				}
			],
			"index": "index.html",
			"main": "main.ts",
			"polyfills": "polyfills.ts",
			"prefix": "app",
			"styles": [
				"../node_modules/bootstrap/dist/css/bootstrap.css",
				"../node_modules/font-awesome/css/font-awesome.css",
				"assets/less/default.theme.less",
				"../node_modules/icheck/skins/flat/blue.css",
				"../node_modules/bootstrap-datepicker/dist/css/bootstrap-datepicker3.css",
				"../node_modules/admin-lte/plugins/daterangepicker/daterangepicker.css",
				"../node_modules/bootstrap3-wysihtml5-bower/dist/bootstrap3-wysihtml5.css",
				"../node_modules/jsoneditor/dist/jsoneditor.min.css",
				"assets/css/styles.css"
			],
			"scripts": [
				"../node_modules/jquery/dist/jquery.js",
				"../node_modules/jqueryui/jquery-ui.js",
				"../node_modules/bootstrap/dist/js/bootstrap.js",
				"../node_modules/moment/moment.js",
				"../node_modules/daterangepicker/daterangepicker.js",
				"../node_modules/bootstrap-datepicker/js/bootstrap-datepicker.js",
				"../node_modules/bootstrap3-wysihtml5-bower/dist/bootstrap3-wysihtml5.all.js",
				"../node_modules/icheck/icheck.js",
				"../node_modules/admin-lte/dist/js/app.js",
				"../node_modules/jsoneditor/dist/jsoneditor.min.js",
				"assets/js/adminLTEOptions.js",
				"assets/js/scripts.js"
			],
			"environmentSource": "environments/environment.ts",
			"environments": {
				"dev": "environments/environment.manager.ts",
				"desenv-manager": "environments/environment.desenv.manager.ts",
				"hom-manager": "environments/environment.hom.manager.ts",
				"staging-manager": "environments/environment.staging.manager.ts",
				"sandbox-manager": "environments/environment.sandbox.manager.ts",
				"prod-manager": "environments/environment.prod.manager.ts"
			}
		}
	],
	"defaults": {
		"styleExt": "css",
		"component": {
			"spec": false,
			"inlineStyle": true
		},
		"service": {
			"spec": false
		},
		"serve": {
			"port": 4201
		}
	}
}
